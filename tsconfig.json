{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "strictPropertyInitialization": false,
    "typeRoots": ["types", "./node_modules/@types"]
  }
}
